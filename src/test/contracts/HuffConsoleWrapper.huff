#include "../../Console.huff"

#define function logTest(uint256, uint256, uint256) pure returns (uint256)

#define macro MAIN() = {
    0x00 calldataload 0xE0 shr              // [sig]
    dup1 __FUNC_SIG(logTest) eq logTestJump jumpi
    0x00 0x00 revert
    logTestJump:
        LOG_TEST()
}

